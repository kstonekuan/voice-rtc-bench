# Unified Echo Agent Configuration
# Copy this file to .env and fill in your credentials

# Daily Configuration (optional - leave blank to disable)
DAILY_ROOM_URL=

# LiveKit Configuration (optional - leave all blank to disable)
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Optional: Custom agent names
DAILY_AGENT_NAME=unified-daily-echo
LIVEKIT_AGENT_NAME=unified-livekit-echo

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
